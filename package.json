{
  "name": "html2txt",
  "description": "html2text but in node",
  "version": "2.0.10",
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/zacanger"
  },
  "license": "LGPL-3.0",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "preferGlobal": true,
  "bin": "./index.js",
  "scripts": {
    "build": "tsc",
    "prebuild": "npm t",
    "preversion": "npm run build",
    "test": "run-s -s test:ts test:lint",
    "test:lint": "eslint -c .eslintrc.json src",
    "test:ts": "tsc --noEmit"
  },
  "homepage": "https://github.com/zacanger/html2txt#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/html2txt.git"
  },
  "bugs": "https://github.com/zacanger/html2txt/issues",
  "keywords": [
    "html",
    "html2text",
    "html2txt",
    "markdown",
    "text"
  ],
  "dependencies": {
    "node-fetch": "2.6.1",
    "to-markdown": "3.1.1",
    "wordwrap": "1.0.0",
    "zeelib": "11.1.8"
  },
  "devDependencies": {
    "@types/node": "14.14.12",
    "@types/node-fetch": "2.5.7",
    "eslint": "7.15.0",
    "eslint-plugin-zacanger": "2.2.0",
    "husky": "4.3.5",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "sortpack": "2.1.11",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ],
    "package.json": [
      "sortpack"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
