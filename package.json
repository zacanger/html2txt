    "zeelib": "11.1.3"
  },
  "devDependencies": {
    "eslint": "7.8.1",
    "eslint-plugin-zacanger": "2.0.7",
    "husky": "4.2.5",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "sortpack": "2.1.7"
